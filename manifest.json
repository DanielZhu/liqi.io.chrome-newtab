{
    "manifest_version": 2,

    "name": "__MSG_appName__",
    "short_name": "liqi.io · 灵感生成器",
    "description": "__MSG_appDesc__",
    "version": "0.2.0",

    "default_locale": "zh_CN",
    "author": "Daniel Zhu(zhumengdan)",
    "background": {
        "scripts": ["src/libs/tj.js", "src/libs/ajax.js", "src/libs/idb.js", "eventPage.js"],
        "persistent": false
    },
    "icons": {
        "16": "./src/assets/images/logo@16w.png",
        "32": "./src/assets/images/logo@32w.png",
        "48": "./src/assets/images/logo.png",
        "128": "./src/assets/images/logo.png"
    },
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "permissions": [
        "tabs",
        "alarms",
        "background",
        "http://*.liqi.io/"
    ],
    "web_accessible_resources": [
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com 'unsafe-eval'; object-src 'self';"
}

